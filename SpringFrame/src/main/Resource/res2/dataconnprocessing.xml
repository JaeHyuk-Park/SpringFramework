<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dated">
	<insert id="insert" parameterType="learnup_pm_model.board">
		insert into board(name, title, nickname) values(#{name}, #{title}, #{nickname});
	</insert>
	
	<select id="full" resultType="learnup_pm_model.board">
		select*from board;
	</select>
	
	<delete id="delete" parameterType="Integer">
		delete from board where num = #{num};
	</delete>
	
	<select id="logincheck" parameterType="learnup_pm_model.logincheck" resultType="learnup_pm_model.logincheck">
		select*from information where email = #{email};
	</select>
</mapper>